@model DirectAgents.Domain.Entities.AB.ClientAccount

<fieldset>
    <legend>Edit ClientAccount Details</legend>
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary()
        @Html.HiddenFor(m => m.Id)
        @Html.HiddenFor(m => m.ClientId)
        <div style="float:left">
            <table>
                <tr>
                    <td>@Html.LabelFor(m => m.ExtCredit)</td>
                    <td>@Html.EditorFor(m => m.ExtCredit)</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(m => m.IntCredit)</td>
                    <td>@Html.EditorFor(m => m.IntCredit)</td>
                </tr>
            </table>
            <br />
            <input type="submit" value="Save" />
            &nbsp;@Html.ActionLink("cancel", "Index", "Dashboard")
        </div>
    }
</fieldset>
