@model IEnumerable<DirectAgents.Domain.Entities.TD.ExtAccount>

<h2>External Accounts</h2>

<table>
<tr>
    <th>Platform</th>
    <th>AccountName</th>
    <th>ExternalId</th>
</tr>
@foreach (var acct in Model)
{
<tr>
    <td>@acct.Platform.Name</td>
    <td>@acct.Name</td>
    <td>@acct.ExternalId</td>
</tr>
}
</table>

<br />
@Html.ActionLink("Stats for all external accounts", "Account", "Stats")