@model IEnumerable<ClientPortal.Data.Entities.TD.DBM.InsertionOrder>

@{
    //ViewBag.Title = "";
    var ajaxOptions = new AjaxOptions()
    {
        UpdateTargetId = "tdAccountsTab",
        HttpMethod = "Get"
    };
}

Show:
@Ajax.ActionLink("Accounts", "TradingDeskAccounts", ajaxOptions)
<br /><br />

<table border="1">
    <tr>
        <th>Id</th>
        <th>Insertion Order</th>
        <th>Account</th>
    </tr>
@foreach (var io in Model)
{
    <tr>
        <td>@io.InsertionOrderID</td>
        <td>@io.InsertionOrderName</td>
        <td>
            @if (io.TradingDeskAccount != null) {
                <text>created</text>
            } else {
                @Ajax.ActionLink("create", "CreateAccount", new { ioID = io.InsertionOrderID }, ajaxOptions)
            }
        </td>
    </tr>
}
</table>