                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT DISTINCT publisher_name, NetTerms, CurrName
FROM            (SELECT        TOP (100) PERCENT CASE WHEN Pub.name IS NULL THEN Aff.name ELSE Pub.name END AS publisher_name, NTT.name AS NetTerms, 
                                                    Curr.name AS CurrName
                          FROM            Item INNER JOIN
                                                    Affiliate AS Aff ON Item.affid = Aff.affid INNER JOIN
