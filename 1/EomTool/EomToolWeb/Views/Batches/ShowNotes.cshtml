@model IEnumerable<BatchNote>

@foreach (var note in Model) {
<p>
    <b>Date:</b> @note.date_created<br />
    <b>By:</b> @note.author<br />
    @if (!String.IsNullOrWhiteSpace(note.extra)) { <b>For:</b> @note.extra<br /> }
    <b>Action:</b> @(note.MediaBuyerApprovalStatus != null ? note.MediaBuyerApprovalStatus.name : "")<br />
    @Html.Raw(note.note.Replace("\n","<br />"))
</p>
}